        <xs:sequence>
            <xs:element name="id" type="eCH-0261:identifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A unique identifier</xs:documentation>
                    <xs:documentation xml:lang="de">Identifikator</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="descriptor" type="eCH-0261:descriptorType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A multi-lingual description</xs:documentation>
                    <xs:documentation xml:lang="de">Bezeichnung</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="zoologicalAnimal" type="eCH-0261:identifierType" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">An animal from a list based on zoological taxonomy, i.e., not a list
