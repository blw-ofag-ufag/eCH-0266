    <xs:complexType name="nutrientNeedOrExcretionType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Nutrient need or excretion definition</xs:documentation>
            <xs:documentation xml:lang="de">Nährstoffbedarf/-ausscheidung</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="eCH-0261:identifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A unique identifier</xs:documentation>
                    <xs:documentation xml:lang="de">Identifikator</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="descriptor" type="eCH-0261:descriptorType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A multi-lingual description</xs:documentation>
                    <xs:documentation xml:lang="de">Bezeichnung</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="animalSpecific" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Indication whether the entry is applicable only to animals.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Angabe, ob der Eintrag tierspezifisch ist.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="required" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Indication whether the declaration of the substance is obligatory.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Angabe, ob die Deklaration der Substanz erforderlich ist.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="substance" type="eCH-0261:identifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The id of a substance referencing eCH-0263</xs:documentation>
                    <xs:documentation xml:lang="de">ID einer Substanz gemäss eCH-0263</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="density" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Density</xs:documentation>
                    <xs:documentation xml:lang="de">Raumgewicht</xs:documentation>
                </xs:annotation>
            </xs:element>
            <!-- Lars: Here we are using an explicit derived measurementType and unitType.
               I would be interested to know if you concur: I think this isn't great but better
               than copy-pasting an entire definition from of measurementType from 0261.
               If you agree, we could delete measurementType in this file
               and eliminate references to it such as birthWeight and slaughterWeight, above.
               Then we would a similar approach in which we require a measurementType
               restricted to kg for those attributes.
            -->
            <xs:element name="quantity" type="eCH-0266:nutrientNeedsMeasurementType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The quantity</xs:documentation>
                    <xs:documentation xml:lang="de">Menge</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!--Livestock-->
    <xs:complexType name="previousYearLivestockSummaryReportType">
        <xs:annotation>
            <xs:documentation xml:lang="en">This defines the animal stock per location and reporting source on
                year-round facilities. The report is for the previousyear vis-à-vis the report date.
            </xs:documentation>
            <xs:documentation xml:lang="de">Dieses Element beschreibt einen Bericht über den Tierbestand pro Standort
                und Berichtsquelle für ganzjährige Einrichtungen. Der Bericht bezieht sich auf das Vorjahr im Vergleich
                zum Berichtsdatum.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="animalStock" type="eCH-0266:animalStockType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="animalStockType">
        <xs:annotation>
            <xs:documentation xml:lang="en">This elements contains the data of an animal cateogory for the given local
                unit.
            </xs:documentation>
            <xs:documentation xml:lang="de">Dieses Element enthält alle Daten einer Tierkategorie der angegebenen
                lokalen Einheit.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="contributionYear" type="xs:gYear">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The year for which the data is reported. In practice this will be
                        the year previous to that of the report date.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält das Jahr, in welche die Daten erhoben wurden.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="reportDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The date on which the report was created or on which the data
                        transmission was effected.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält das Datum des Berichts bzw. der Datenübertragung.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="mainLocalUnit" type="eCH-0108:localIdType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The id of localUnit.</xs:documentation>
                    <xs:documentation xml:lang="de">BUR-Nr. der lokalen Einheit.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <!-- Lars: I am seeing some problems with our documentation here:
               Firstly: I think the description is incorrect. We should be using the BUR-NR, not the kt_id_b (the cantonal id) in the standard!
            Secondly, a "Betriebsform" or farmType from 0261 is not a localUnit at all.  -->
            <xs:element name="surveyLocalUnit">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contains the cantonal id of the farm type at the level at which the
                        data in this report was collected.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die kantonale Identifikationsnummer der Betriebsform auf der
                        Stufe, auf der die sich in diesem Report befindlichen Daten erfasst wurden.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="responsiblePerson" type="eCH-0261:personIdentifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The person creating the report and/or who is responsible for the
                        localUnit.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Person, die den Bericht erstellt und/oder für die lokale Einheit
                        verantwortlich ist.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:element name="animalCategoryStructural" type="eCH-0261:identifierType">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">An id referencing an entry in the animal category based on
                            structural data.
                        </xs:documentation>
                        <xs:documentation xml:lang="de">Tierkategorie gemäss Direktzah-lungsvollzug Landwirtschaft
                            (Merkmalskatalog Strukturdaten).
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="animalCategoryPRIF" type="eCH-0261:identifierType">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">An id referencing an animal category based on PRIF
                        </xs:documentation>
                        <xs:documentation xml:lang="de">Tierkategorie gemäss GRUD</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
            <xs:choice>
                <xs:element name="livestockAnimalTracing" type="eCH-0266:livestockReportedViaAnimalTracingType"/>
                <xs:element name="livestockSelfDeclared" type="eCH-0266:selfReportedLivestockType"
                            maxOccurs="unbounded"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="livestockReportedViaAnimalTracingType">
        <xs:annotation>
            <xs:documentation xml:lang="en">This node contains information regarding cattle, equidae, sheep, goats, and
                bison. Do not use if the animal in question belongs to another category.
            </xs:documentation>
            <xs:documentation xml:lang="de">Enthält die TVD-Daten für Rindvieh, Tiere der Pferdegattung, Schafe, Ziegen
                und Bisons. Für die anderen Tierkategorien darf dieser Knoten nicht übermittelt werden.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="animalTracingDataOriginal" type="eCH-0266:livestockReportViaAnimalTracingOriginalType"/>
            <xs:element name="animalTrackingDataFinal" type="eCH-0266:livestockReportedViaAnimalTracingFinalType"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="livestockReportViaAnimalTracingOriginalType">
        <xs:annotation>
            <xs:documentation xml:lang="en">This element describes the original data of the animal stock from the ATD.
            </xs:documentation>
            <xs:documentation xml:lang="de">Dieses Element beschreibt die Originaldaten des Tierbestands aus der TVD.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="quantityTotal" type="xs:nonNegativeInteger">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contains the total quantity of the respective animal category 
                        consult regulation for formula.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält den gesamten Bestand der Tierkategorie. (Gezählte Tiertage
                        auf dem Betrieb + Tiertage Sömmerung Inland + Tiertage auf angestammten Sömmerungsbetrieben in
                        der ausländischen Grenzzone) / Tage des Jahres (Summe der Werte aus durationSum + livestockUnkt
                        + quantity) /365 (bzw. 366 im Falle eines Schaltjahres)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="livestockUnitTotal">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contains the quantity of livestock units at the operation as well as
                        those at summering locations, both domestic (inland) and those in the foreign border zone.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die Summe der Anzahl Grossvieheinheiten auf dem Betrieb
                        sowie der Sömmerung im Inland und auf angestammten Sömmerungsbetrieben in der ausländischen
                        Grenzzone.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:fractionDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="durationSumTotal" type="xs:nonNegativeInteger">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contains the sum of the days animals were located at an operation or
                        at summering locations, both domestic (inland) and those in the foreign border zone.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die Summe der Tiertage auf dem Betrieb sowie der Sömmerung
                        im Inland und auf angestammten Sömmerungsbetrieben in der ausländischen Grenzzone.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="durationSumTotalCorrectedNutrientBalance" type="xs:nonNegativeInteger" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Result after subtracting nutrient balance. See regulation for
                        details.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Ergebnis nach Abzug Nährstoffbilanz. Für die Berechnung der
                        Nährstoffbilanz kann ein Betrieb Abzüge für vorübergehend abwesende Tiere vornehmen. Gründe
                        dafür sind Alpung, Abwesenheit oder Pensionsvieh.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="surveyFarmDetails" type="eCH-0266:surveyFarmDetailsType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Enthält die TVD-Daten zum Tierbestand auf dem Hauptbetrieb.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="summeringInlandDetails" type="eCH-0266:summeringInlandDetailsType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">ATD data for summering at domestic locations (inland). See
                        regulation for details.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Dieser Knoten enthält die TVD-Daten zur Sömmerung im Inland. Dieser
                        Knoten ist zwingend zu liefern, wenn Tiere im Inland gesömmert wurden (ohne Bisons).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="summeringAbroadDetails" type="eCH-0266:summeringAbroadDetailsType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">ATD data for summering in foreign border zones. See regulation for
                        details.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Dieser Knoten enthält die TVD-Daten zur Sömmerung auf angestammten
                        Sömmerungsbetrieben in der ausländischen Grenzzone (ohne Bisons). Dieser Knoten ist zwingend zu
                        liefern, wenn Tiere auf angestammten Sömmerungsbetrieben in der ausländischen Grenzzone
                        gesömmert wurden (ohne Bisons).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="surveyFarmDetailsType">
        <xs:annotation>
            <xs:documentation xml:lang="en">ATD data for the main operation.</xs:documentation>
            <xs:documentation xml:lang="de">Enthält die TVD-Daten zum Tierbestand auf dem Hauptbetrieb.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- recommend renaming in V2: declarationDateQuantity  -->
            <xs:element name="declarationDayQuantity" type="xs:nonNegativeInteger">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Stock of the animal category as of the declaration date.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält den Bestand der Tierkategorie am Stichtag.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="quantity">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The quantity of animals from ATD. See regulation for details.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die Anzahl Tiere auf dem Betrieb aus den TVD-Daten. Gezählte
                        Tiertage auf dem Betrieb / Tage des Jahres (Basisinformation für Nährstoffbilanz)
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:fractionDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="livestockUnit">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Livestock unit from the ATD. See regulation for details.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die Anzahl GVE auf dem Betrieb aus den TVD-Daten. Gezählte
                        Tiertage / Tage des Jahres * GVE-Faktor
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:fractionDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="durationSum" type="xs:nonNegativeInteger" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The number of days animals at location from the ATD. See regulation
                        for details.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die Anzahl Tiertage auf dem Betrieb aus den TVD-Daten (inkl.
                        Aufenthalte an Ausstellungen, Tierklinik). Auf der TVD gezählte Aufenthaltstage
                        (Übernachtungsregel).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="summeringInlandDetailsType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Dieses Element enthält die TVD-Daten zur Sömmerung im Inland.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="livestockUnit">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The quantity of livestock units in domestic summering. See
                        regulation for details.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die Anzahl im Inland gesömmerte GVE aus den TVD-Daten.
                        Gezählte Tiertage der Sömmerung im Inland / Tage des Jahres * GVE-Faktor
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:fractionDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="normedLivestockUnit">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Domestically summered "Normalstösse" livestock units from ATD. See
                        regulation for details.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die Anzahl Normalstösse der im Inland gesömmerten Tiere aus
                        den TVD-Daten.→ Gezählte Tiertage der Sömmerung im Inland* GVE-Faktor / 100.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:fractionDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="durationSum" type="xs:nonNegativeInteger" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Summering days at domestic (inland) locations from ATD.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die Anzahl Sömmerungstage im Inland aus den TVD-Daten.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- recommendation V2: rename to summeringForeignDetailsType. "Abroad" has the connotation of "Übersee" -->
    <xs:complexType name="summeringAbroadDetailsType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Summering days at non-domestic border zone locations from the ATD.
            </xs:documentation>
            <xs:documentation xml:lang="de">Dieses Element enthält die TVD-Daten zur Sömmerung auf angestammten
                Sömmerungsbetrieben in der ausländischen Grenzzone.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="livestockUnit">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation xml:lang="en">Livestock unit quantity at non-domestic border zone locations
                            from the ATD. See regulation for details.
                        </xs:documentation>
                        <xs:documentation xml:lang="de">Enthält die auf angestammten Sömmerungsbetrieben in der
                            ausländischen Grenzzone gesömmerte GVE aus den TVD-Daten. Gezählte Tiertage der Sömmerung
                            auf angestammten Sömmerungsbetrieben in der ausländischen Grenzzone / Tage des Jahres *
                            GVE-Faktor
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:fractionDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="durationSum" type="xs:nonNegativeInteger" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Summering duration sum at non-domestic border zone locations from
                        the ATD.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die Anzahl Sömmerungstage auf angestammten
                        Sömmerungsbetrieben in der ausländischen Grenzzone aus den TVD-Daten.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="livestockReportedViaAnimalTracingFinalType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Animal tracing data that was corrected by the Canton.</xs:documentation>
            <xs:documentation xml:lang="de">Enthält die finalen Daten (nachKorrekturen vom Kanton).</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="quantityTotal" type="xs:nonNegativeInteger">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contains the entire stock of the animal category after correction by
                        the canton. See regulation for details.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält den gesamten Bestand der Tierkategorie nach Korrektur Kanton
                        (Tiertage Ganzjahresbetrieb + Tiertage Sömmerung Inland + Tiertage Sömmerung Ausland angestammt)
                        / Tage des Jahres.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="livestockUnitTotal">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation xml:lang="en">Contains the sum of the number of large livestock units on the
                            holding as well as the summer-grazing in the domestic and on traditional summer-grazing
                            farms in the foreign border zone after correction by the Canton.
                        </xs:documentation>
                        <xs:documentation xml:lang="de">Enthält die Summe der Anzahl Grossvieheinheit auf dem Betrieb
                            sowie der Sömmerung im Inland und auf angestammten Sömmerungsbetrieben in der ausländischen
                            Grenzzone nach Korrektur Kanton.
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:fractionDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="surveyFarmDetails" type="eCH-0266:surveyFarmDetailsType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contains the TVD data of the animal stock at the main farm after
                        correction by the canton.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die TVD-Daten nach Korrektur Kanton zum Tierbestand auf dem
                        Hauptbetrieb.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="summeringInlandDetails" type="eCH-0266:summeringInlandDetailsType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contains ATD data for summer grazing of cattle and animals of the
                        equine genus at domestic (inland) operations after cantonal correction.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Dieser Knoten enthält die TVD-Daten zur Sömmerung von Rindvieh und
                        Tieren der Pferdegattung im Inland nach Korrektur Kanton.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="summeringAbroadDetails" type="eCH-0266:summeringAbroadDetailsType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contains the ADT summering data at traditional summer-dwelling farms
                        in the foreign border zone after cantonal correction
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Dieser Knoten enthält die TVD-Daten zur Sömmerung auf angestammten
                        Sömmerungsbetrieben in der ausländischen Grenzzone nach Korrektur Kanton.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="selfReportedLivestockType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Constains self-declared data. See regulation for details/formula.
            </xs:documentation>
            <xs:documentation xml:lang="de">Dieser Knoten enthält die Daten aus der Selbstdeklaration. → Kann pro
                Tierkategorie mehrmals übermittelt werden, falls Tiere unterschiedlich lange gesömmert wurden. Gibt es
                nur eine einzige Sömmerungsdauer (2.1.7.5summeringDuration), wird pro Tierkategorie nur ein Datensatz
                mit allen Angaben geliefert. Gibt es mehrere Sömmerungsdauern, werden mehrere Datensätze geliefert: ein
                Datensatz enthält die Anzahl Tiere am Stichtag und den Durchschnittswert (2.1.7.1 + 2.1.7.2) (als Anzahl
                gesömmerte Tiere und Sömmerungsdauer wird 0 geliefert)• weitere Datensätze mit der jeweiligen Anzahl der
                gesömmerten Tiere und der dazugehörenden Sömmerungsdauer (2.1.7.4 + 2.1.7.5) (als Anzahl Tiere am
                Stichtag und Durchschnittswert wird 0 geliefert)
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- Suggested name change for V2: declarationDateQuantity -->
            <xs:element name="declarationDayQuantity" type="xs:nonNegativeInteger">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contains the stock of the animal category as of the reference
                        date.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält den Bestand der Tierkategorie am Stichtag.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="averageQuantity" type="xs:nonNegativeInteger">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contains the average stock (unit=piece) of the animal category from
                        the previous year.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält den Durchschnittsbestand der Tierkategorie in Stück im
                        Vorjahr.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="detentionForm" type="eCH-0261:identifierType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contains the detention form for pigs and poultry and is only
                        mandatory for these genera. For other genera this information should not be provided.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die Haltungsform für Schweine und Geflügel und ist nur für
                        diese Gattungen zwingend. Für andere Gattungen soll diese Information nicht geliefert werden.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="summeringQuantity" type="xs:nonNegativeInteger">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Contains the number (unit=piece) of flocked animals in the previous
                        year.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die Anzahl (Stück) der gesömmerten Tiere im Vorjahr.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="summeringDuration" type="xs:nonNegativeInteger">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The summering period of the previous year measured in days.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Enthält die Sömmerungsdauer im Vorjahr in Anzahl Tagen.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!--MeasurementType redefinition from eCH-0261-->
    <!-- Lars: is there any way of simply inheriting this instead of a full copy-paste? -->
    <xs:complexType name="measurementType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Messung mit Wert und Einheit</xs:documentation>
            <xs:documentation xml:lang="fr">Mesure avec valeur et unité</xs:documentation>
            <xs:documentation xml:lang="en">Measurement with value and unit</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="value" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Menge, Wert</xs:documentation>
                    <xs:documentation xml:lang="fr">Quantité, valeur</xs:documentation>
                    <xs:documentation xml:lang="en">Quantity, value</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="unit" type="eCH-0261:unitType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Zugehörige Einheit</xs:documentation>
                    <xs:documentation xml:lang="fr">Unité associée</xs:documentation>
                    <xs:documentation xml:lang="en">Associated unit</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="unitName" type="xs:token" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Genauere Bezeichnung der Einheit, wenn im Feld «unit» die Einheit
                        «pc» (Stück) angegeben wurde.
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">Désignation plus précise de l'unité, si l'unité "pc" (pièce) a été
                        indiquée dans le champ "unit".
                    </xs:documentation>
                    <xs:documentation xml:lang="en">More precise designation of the unit if the unit "pc" (piece) was
                        specified in the "unit" field.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!--Root-Element-->
    <xs:element name="root">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="individualAnimal" type="eCH-0266:individualAnimalType"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
