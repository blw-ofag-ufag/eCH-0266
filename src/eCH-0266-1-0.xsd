            </xs:enumeration>
            <xs:enumeration value="M">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Monat(e)</xs:documentation>
                    <xs:documentation xml:lang="de">Monat(e)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Y">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Jahr(e)</xs:documentation>
                    <xs:documentation xml:lang="de">Jahr(e)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <!-- Lars, this is new -->
    <xs:complexType name="ageRestrictionType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Age restriction</xs:documentation>
            <xs:documentation xml:lang="de">Alterseinschränkung</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="code" type="eCH-0261:identifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A unique identifier</xs:documentation>
                    <xs:documentation xml:lang="de">Identifikator</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="descriptor" type="eCH-0261:descriptorType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A multi-lingual description</xs:documentation>
                    <xs:documentation xml:lang="de">Bezeichnung</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="lowerLimit" type="xs:integer" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Lower age limit</xs:documentation>
                    <xs:documentation xml:lang="de">Unteres Alterslimit</xs:documentation>
                </xs:annotation>
            </xs:element>
            <!-- Lars, I extracted out the operator restriction or enumeration into its own type -->
            <xs:element name="lowerOperator" type="eCH-0266:limitOperatorType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Operator for the lower age limit</xs:documentation>
                    <xs:documentation xml:lang="de">Operator für das untere Alterslimit</xs:documentation>
                </xs:annotation>
            </xs:element>
            <!-- Lars, I extracted out the calendar unit restriction or enumeration into its own type -->
            <xs:element name="lowerUnit" type="eCH-0266:limitCalendarUnitType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The calendar unit for the lower limit</xs:documentation>
                    <xs:documentation xml:lang="de">Einheit für das untere Alterslimit</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="upperLimit" type="xs:integer" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The calendar unit for the upper limit</xs:documentation>
                    <xs:documentation xml:lang="de">Oberes Alterslimit</xs:documentation>
                </xs:annotation>
            </xs:element>
            <!-- Lars, I extracted out the operator restriction or enumeration into its own type -->
            <xs:element name="upperOperator" type="eCH-0266:limitOperatorType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Operator for the upper age limit</xs:documentation>
                    <xs:documentation xml:lang="de">Operator für das obere Alterslimit</xs:documentation>
                </xs:annotation>
            </xs:element>
            <!-- Lars, I extracted out the calendar unit restriction or enumeration into its own type -->
            <xs:element name="upperUnit" type="eCH-0266:limitCalendarUnitType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The calendar unit for the lower limit</xs:documentation>
                    <xs:documentation xml:lang="de">Einheit für das untere Alterslimit</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="animalWelfareContributionType">
