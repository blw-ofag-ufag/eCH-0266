<?xml version="1.0" encoding="UTF-8"?>
<!-- Lars: removed attributeFormDefault="unqualified" as it is redundant-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:eCH-0108="http://www.ech.ch/xmlns/eCH-0108/7"
           xmlns:eCH-0261="http://www.ech.ch/xmlns/eCH-0261/1" xmlns:eCH-0266="http://www.ech.ch/xmlns/eCH-0266/1"
           targetNamespace="http://www.ech.ch/xmlns/eCH-0266/1" elementFormDefault="qualified" version="1.0">
    <xs:annotation>
        <xs:documentation xml:lang="de">Nutztierhaltung - Entwurf, Stand 03.05.2024</xs:documentation>
    </xs:annotation>
    	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0261/1"/>
    	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0108/7"/>
    <!-- Main data types -->
    <xs:complexType name="individualAnimalType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Einzeltier</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="eCH-0261:identifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A unique identifier</xs:documentation>
                    <xs:documentation xml:lang="de">Identifikator</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Name of the animal</xs:documentation>
                    <xs:documentation xml:lang="de">Name des Tiers</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="utilisation" type="eCH-0261:identifierType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The utilisation (for what purpose kept or reared)</xs:documentation>
                    <xs:documentation xml:lang="de">Nutzungsausrichtung</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="zoologicallAnimal" type="eCH-0261:identifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">An animal from a list based on zoological taxonomy, i.e., not a list
                        based on a regulation.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Ein Tier aus einer Liste gemäss zoologischer Taxonomie, d.h. nicht
                        aus einer verordungsbasierten Liste.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="animalCategoryPRIF" type="eCH-0261:identifierType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">An animal category according to PRIF</xs:documentation>
                    <xs:documentation xml:lang="de">Tierkategorie nach Grud</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="animalCategoryStructural" type="eCH-0261:identifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">An animal category from a structural perspective.</xs:documentation>
                    <xs:documentation xml:lang="de">Tierkategorie nach Strukturdaten</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateOfBirth" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The date of birth</xs:documentation>
                    <xs:documentation xml:lang="de">Geburt</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:choice>
                        <xs:element name="yearMonthDay" type="xs:date"/>
                        <xs:element name="yearMonth" type="xs:gYearMonth"/>
                    </xs:choice>
                </xs:complexType>
            </xs:element>
            <xs:element name="dateOfDeath" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The date of death</xs:documentation>
                    <xs:documentation xml:lang="de">Tod</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:choice>
                        <xs:element name="yearMonthDay" type="xs:date"/>
                        <xs:element name="yearMonth" type="xs:gYearMonth"/>
                    </xs:choice>
                </xs:complexType>
            </xs:element>
            <xs:element name="birthWeight" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The birth weight</xs:documentation>
                    <xs:documentation xml:lang="de">Geburtsgewicht</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="eCH-0266:measurementType">
                            <xs:sequence>
                                <xs:element name="value">
                                    <xs:simpleType>
                                        <xs:restriction base="xs:decimal"/>
                                    </xs:simpleType>
                                </xs:element>
                                <xs:element name="unit">
                                    <xs:simpleType>
                                        <xs:restriction base="eCH-0261:unitType">
                                            <xs:enumeration value="kg"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:element>
                            </xs:sequence>
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="livingWeight" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The live weight</xs:documentation>
                    <xs:documentation xml:lang="de">Lebendgewicht</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="eCH-0266:measurementType">
                            <xs:sequence>
                                <xs:element name="value">
                                    <xs:simpleType>
                                        <xs:restriction base="xs:decimal"/>
                                    </xs:simpleType>
                                </xs:element>
                                <xs:element name="unit">
                                    <xs:simpleType>
                                        <xs:restriction base="eCH-0261:unitType">
                                            <xs:enumeration value="kg"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:element>
                            </xs:sequence>
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="slaughterWeight" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Slaughter weight in kilograms</xs:documentation>
                    <xs:documentation xml:lang="de">Schlachtgewicht in Kilogramm</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="eCH-0266:measurementType">
                            <xs:sequence>
                                <xs:element name="value">
                                    <xs:simpleType>
                                        <xs:restriction base="xs:decimal"/>
                                    </xs:simpleType>
                                </xs:element>
                                <xs:element name="unit">
                                    <xs:simpleType>
                                        <xs:restriction base="eCH-0261:unitType">
                                            <xs:enumeration value="kg"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:element>
                            </xs:sequence>
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="location" type="eCH-0108:localIdType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A location, facility, site, or operation</xs:documentation>
                    <xs:documentation xml:lang="de">Standort</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice minOccurs="0">
                <xs:element name="owner" type="eCH-0108:uidType">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">An owner (identified by UID)</xs:documentation>
                        <xs:documentation xml:lang="de">Eigentümer (UID)</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="ownerPerson" type="eCH-0261:personIdentifierType">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">Owner (An identifier of a person without a UID)
                        </xs:documentation>
                        <xs:documentation xml:lang="de">Eigentümer (Personenidentifikator für Personen ohne UID)
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
            <xs:element name="biologicalSex" type="eCH-0266:biologicalSexType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Biological sex</xs:documentation>
                    <xs:documentation xml:lang="de">Geschlecht</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="referenceDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Release date of the record</xs:documentation>
                    <xs:documentation xml:lang="de">Abrufdatum des Datensatzes</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="animalGroupType">
        <xs:annotation>
            <xs:documentation xml:lang="en">A collection of animals (individual, structural, or PRIF).
            </xs:documentation>
            <xs:documentation xml:lang="de">Tiergruppe (einzeln, Struktur, GRUD)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="eCH-0261:identifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Identifier</xs:documentation>
                    <xs:documentation xml:lang="de">Identifikator</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:element name="individualAnimal" type="eCH-0261:identifierType" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">Individual animal</xs:documentation>
                        <xs:documentation xml:lang="de">Einzeltiere</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:sequence>
                    <xs:element name="animalCategoryPRIF" type="eCH-0261:identifierType" minOccurs="0">
                        <xs:annotation>
                            <!-- Lars/Dani: I think "und/oder" should be "oder" -->
                            <xs:documentation xml:lang="en">Animal category: if individual animals are not listed, a and
                                animal category based on PRIF and/or based on a structural animal category
                                (Merkmalskatalog) must be provided.
                            </xs:documentation>
                            <xs:documentation xml:lang="de">Tierkategorie: Werden keine Einzeltiere angegeben, muss eine
                                Kategorie gemäss GRUD und/oder eine Kategorie gemäss Strukturdatenerhebung
                                (Merkmalskatalog) angegeben werden.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="animalCategoryStructural" type="eCH-0261:identifierType" minOccurs="0">
                        <xs:annotation>
                            <!-- Lars/Dani: I think "und/oder" should be "oder" -->
                            <xs:documentation xml:lang="en">Animal category: if individual animals are not listed, a and
                                animal category based on PRIF and/or based on a structural animal category
                                (Merkmalskatalog) must be provided.
                            </xs:documentation>
                            <xs:documentation xml:lang="de">Tierkategorie: Werden keine Einzeltiere angegeben, muss eine
                                Kategorie gemäss GRUD und/oder eine Kategorie gemäss Strukturdatenerhebung
                                (Merkmalskatalog) angegeben werden.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:choice>
            <xs:element name="quantity" type="xs:integer">
                <xs:annotation>
                    <xs:documentation xml:lang="en">the quantity of animals in the group</xs:documentation>
                    <xs:documentation xml:lang="de">Anzahl Tiere in der Gruppe</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="utilisation" type="eCH-0261:identifierType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Utilisation</xs:documentation>
                    <xs:documentation xml:lang="de">Nutzungsausrichtung</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sumLivingWeight" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The sum of the live weight in kilograms</xs:documentation>
                    <xs:documentation xml:lang="de">Summe Lebendgewicht in Kilogramm</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="eCH-0266:measurementType">
                            <xs:sequence>
                                <xs:element name="value">
                                    <xs:simpleType>
                                        <xs:restriction base="xs:decimal"/>
                                    </xs:simpleType>
                                </xs:element>
                                <xs:element name="unit">
                                    <xs:simpleType>
                                        <xs:restriction base="eCH-0261:unitType">
                                            <xs:enumeration value="kg"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:element>
                            </xs:sequence>
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="sumSlaughterWeight" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The sum of the slaughter weight in kilograms</xs:documentation>
                    <xs:documentation xml:lang="de">Summe Schlachtgewicht in Kilogramm</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="eCH-0266:measurementType">
                            <xs:sequence>
                                <xs:element name="value">
                                    <xs:simpleType>
                                        <xs:restriction base="xs:decimal"/>
                                    </xs:simpleType>
                                </xs:element>
                                <xs:element name="unit">
                                    <xs:simpleType>
                                        <xs:restriction base="eCH-0261:unitType">
                                            <xs:enumeration value="kg"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:element>
                            </xs:sequence>
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:choice minOccurs="0">
                <xs:element name="owner" type="eCH-0108:uidType">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">The owner (identified by UID)</xs:documentation>
                        <xs:documentation xml:lang="de">Eigentümer (UID)</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="ownerPerson" type="eCH-0261:personIdentifierType">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">Owner (An identifier of a person without a UID)
                        </xs:documentation>
                        <xs:documentation xml:lang="de">Eigentümer (Personenidentifikator für Personen ohne UID)
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
            <xs:element name="location" type="eCH-0108:localIdType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The location of the group</xs:documentation>
                    <xs:documentation xml:lang="de">Standort</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="husbandryForm" type="eCH-0261:identifierType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">An identifier of the husbandry form defining the keeping of the
                        animals.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Identifikator der Tierhaltung in der sich die Tiergruppe aufhält.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="parentGroup" type="eCH-0261:identifierType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The parent group: An animal group can be part of an overarching
                        group. This id identifies that group.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Übergeordnete Gruppe: Eine Tiergruppe kann Teil einer anderen
                        Tiergruppe sein. In diesem Feld kann die ID der übergeordneten Gruppe angegeben werden.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="referenceDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Release date of the record</xs:documentation>
                    <xs:documentation xml:lang="de">Abrufdatum des Datensatzes</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="animalCategoryPRIFType">
        <xs:annotation>
            <xs:documentation xml:lang="en">PRIF animal category (PRIF=Principles of fertilisation of agricultural crops
                in Switzerland)
            </xs:documentation>
            <xs:documentation xml:lang="de">GRUD-Tierkategorie (GRUD=Grundlagen für die Düngung landwirtschaftlicher
                Kulturen in der Schweiz)
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="eCH-0261:identifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Unique identifier</xs:documentation>
                    <xs:documentation xml:lang="de">Identifikator</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="descriptor" type="eCH-0261:descriptorType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A multi-lingual description</xs:documentation>
                    <xs:documentation xml:lang="de">Bezeichnung</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="utilisation" type="eCH-0261:identifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Utilisation</xs:documentation>
                    <xs:documentation xml:lang="de">Nutzungsausrichtung</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="usedInSuisseBilanz" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Indication whether the category is used in the SuisseBilanz
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Angabe, ob die Kategorie in der SuisseBilanz verwendet wird
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="nutrientRequirement" type="eCH-0266:nutrientNeedOrExcretionType" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The nutrient requirement</xs:documentation>
                    <xs:documentation xml:lang="de">Nährstoffbedarf</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="nutrientAccumulation" type="eCH-0266:nutrientNeedOrExcretionType" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The nutrient accumulation</xs:documentation>
                    <xs:documentation xml:lang="de">Nährstoffanfall</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="zoologicalAnimal" type="eCH-0261:identifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">An animal from a list based on zoological taxonomy, i.e., not a list
                        based on a regulation.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Eine Tierkategorie aus einer Liste gemäss zoologischer Taxonomie,
                        d.h. nicht aus einer verordungsbasierten Liste.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <!-- Dani: do we have a better description?  -->
            <xs:element name="rotation" type="eCH-0266:rotationType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Rotation</xs:documentation>
                    <xs:documentation xml:lang="de">Umtrieb</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="animalCategoryStructuralType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Animal category based on structural data</xs:documentation>
            <xs:documentation xml:lang="de">Tierkategorien Strukturdaten</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="eCH-0261:identifierType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Identifier</xs:documentation>
                    <xs:documentation xml:lang="de">Identifikator</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="descriptor" type="eCH-0261:descriptorType">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A multi-lingual description</xs:documentation>
                    <xs:documentation xml:lang="de">Bezeichnung</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="zoologicalAnimal" type="eCH-0261:identifierType" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">An animal from a list based on zoological taxonomy, i.e., not a list
                        based on a regulation.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Eine Tierkategorie aus einer Liste gemäss zoologischer Taxonomie,
                        d.h. nicht aus einer verordungsbasierten Liste.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="animalCategoryPRIF" type="eCH-0261:identifierType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Indicates whether the category is used in the SuisseBilanz
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Angabe, ob die Kategorie in der SuisseBilanz verwendet wird
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="biologicalSex" type="eCH-0266:biologicalSexType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Biological sex</xs:documentation>
                    <xs:documentation xml:lang="de">Geschlecht</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ageRestriction" type="eCH-0261:identifierType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Identifies the age restriction referencing an entry in an external
                        list.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Identifiziert die Altereineinschränkung -- ein Eintrag aus einer
                        Liste
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <!-- should be of type VeracityType from 0261 and restricted to TRUE/FALSE-->
            <xs:element name="offspring" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Indicates the existence of offspring.</xs:documentation>
                    <xs:documentation xml:lang="de">Angabe, ob Nachkommen existieren</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:enumeration value="YES"/>
                        <xs:enumeration value="NO"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="lifeStage" type="eCH-0266:lifeStageType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">General information pertaining to the age of the animal.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Generelle Angabe über das Alter des Tieres.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="synonym" type="eCH-0261:descriptorType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Alternate multi-lingual descriptor for the animal category
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Alternative Bezeichnung(en) für die Tierkategorie für Tierkategorien
                        mit mehreren Bezeichnungen.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="rotation" type="eCH-0266:rotationType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Rotation</xs:documentation>
                    <xs:documentation xml:lang="de">Umtrieb</xs:documentation>
                </xs:annotation>
            </xs:element>
            <!-- LARS is this the correct type and annotation ?  -->
            <xs:element name="ethoCategory" type="eCH-0266:animalWelfareContributionType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">TBD !</xs:documentation>
                    <xs:documentation xml:lang="de">Beitrag zum Tierwohl</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="standardLabourUnitProLivestockUnit">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Standard labour unit per livestock unit</xs:documentation>
                    <xs:documentation xml:lang="de">Standardarbeitskraft pro Grossvieheinheit (SAKProGVE)
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:minInclusive value="0"/>
                        <xs:fractionDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="livestockUnitFactor">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Livestock unit factor</xs:documentation>
                    <xs:documentation xml:lang="de">GVE-Faktor</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:minInclusive value="0"/>
                        <xs:fractionDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="zoologicalAnimalType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Zoological classification of an animal</xs:documentation>
            <xs:documentation xml:lang="de">Zoologische Klassifizierung eines Tieres</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="code">
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:maxLength value="50"/>
