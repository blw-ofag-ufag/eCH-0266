            <xs:documentation xml:lang="en">Animal welfare contribution</xs:documentation>
            <xs:documentation xml:lang="de">Tierwohlbeiträge</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="eCH-0261:identifierType">
                <xs:annotation>
