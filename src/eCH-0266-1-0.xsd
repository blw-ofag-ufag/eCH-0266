    <!-- Lars, this is new -->
    <xs:simpleType name="limitOperatorType">
        <xs:annotation>
            <xs:documentation xml:lang="en">A restricted set of operators than can be used to define a limit.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:enumeration value="&lt;">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Less than</xs:documentation>
                    <xs:documentation xml:lang="de">Kleiner als</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="&lt;=">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Less than or equal to</xs:documentation>
                    <xs:documentation xml:lang="de">Kleiner gleich</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="=">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Equals</xs:documentation>
                    <xs:documentation xml:lang="de">Gleich</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="!=">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Inequality</xs:documentation>
                    <xs:documentation xml:lang="de">Ungleich</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="&gt;=">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Greater than or equal to</xs:documentation>
                    <xs:documentation xml:lang="de">Grösser gleich</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="&gt;">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Greater than</xs:documentation>
                    <xs:documentation xml:lang="de">Grösser als</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <!-- Lars, this is new -->
    <xs:simpleType name="limitCalendarUnitType">
        <xs:annotation>
            <xs:documentation xml:lang="en">A restricted set of calendar units than can be used to define a limit.
