            <xs:documentation xml:lang="en">Biological sex</xs:documentation>
            <xs:documentation xml:lang="de">Geschlecht</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:enumeration value="M">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Male</xs:documentation>
                    <xs:documentation xml:lang="de">Männlich</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="F">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Female</xs:documentation>
                    <xs:documentation xml:lang="de">Weiblich</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="M_OR_F">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Male or female, for use in lists in which the animal in question may
                        be either one or the other.
                    </xs:documentation>
                    <xs:documentation xml:lang="de">Männlich oder weiblich: In einigen Listen sind Tiere aufgeführt, die
                        entweder weiblich oder männlich sein können.
